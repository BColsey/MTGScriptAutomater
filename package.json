{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node app.js",
    "client": "cd client && npm start",
    "production": "concurrently \"npm run server\" \"npm run serve\"",
    "serve": "cd client && serve -s build -p 3000"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "~0.7.4",
    "express": "^4.16.3",
    "express-mongo-db": "^2.0.4",
    "mongodb": "^3.0.10",
    "morgan": "~1.0.0",
    "nodemon": "^1.18.3",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
